{
  "expo": {
    "name": "Vichar Group",
    "slug": "vicharGroup",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nisarg39.vicharGroup"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.nisarg39.vicharGroup",
      "permissions": ["INTERNET"]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png",
      "permissions": ["INTERNET"]
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 500,
          "resizeMode": "contain",
          "backgroundColor": "white"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/BADABB.ttf",
            "./assets/fonts/Tektur-Bold.ttf"
          ]
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "e274dba4-8a28-471d-8c64-b512421b4099"
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ]
  }
}
